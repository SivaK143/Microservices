spring:
  application:
    name: "configserver"
  profiles:
    #active: native
    active: git
  cloud:
    config:
      server:
        #native:
           #search-locations: "classpath:/config"
           #search-locations: "file:///C://Users//sivakrishna//Documents//config"
        git:
          uri: "https://github.com/eazybytes/eazybytes-config.git"
          default-label: main
          timeout: 10   


server:
  port: 8071
